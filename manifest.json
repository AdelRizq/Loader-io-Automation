{
    "name": "Loader.io Automation",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "permissions": ["https://loader.io/tests/*"],
    "content_scripts": [
        {
            "matches": ["https://loader.io/tests/*"],
            "js": ["src/inject.js"],
            "css": ["src/inject.css"]
        }
    ]
}
